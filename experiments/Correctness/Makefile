TARGETS = geosteiner simple smithnew simplesort smithnewsort

all: $(TARGETS)

simple:
	cd Simple; make

simplesort:
	cd SimpleSort; make

geosteiner:
	cd GeoSteiner; make

smithnew:
	cd SmithNew; make

smithnewsort:
	cd SmithNewSort; make

clean:
	cd Simple; make clean
	cd SimpleSort; make clean
	cd GeoSteiner; make clean
	cd SmithNew; make clean
	cd SmithNewSort; make clean
