MAKE = make

simple:
	$(MAKE) -C Simple

simplesort:
	$(MAKE) -C SimpleSort

smithnew:
	$(MAKE) -C SmithNew

smithnewsort:
	$(MAKE) -C SmithNewSort

smithold:
	$(MAKE) -C SmithOld

all: simple simplesort smithnew smithnewsort smithold

clean:
	$(MAKE) -C Simple clean
	$(MAKE) -C SimpleSort clean
	$(MAKE) -C SmithNew clean
	$(MAKE) -C SmithNewSort clean
	$(MAKE) -C SmithOld clean
